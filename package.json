{
  "name": "myblog",
  "version": "2.0.0",
  "description": "张益铭前端小博客 - v2.0",
  "main": "app.js",
  "scripts": {
    "build:dll": "npm run clean && webpack --config ./build/webpack.config.dll.js",
    "jest": "jest",
    "clean": "rimraf ./dist/*",
    "seeSize": "webpack --display-modules --sort-modules-by size",

    "dev:client": "npm run clean && cross-env NODE_ENV=development node ./build/dev/devServer.js",
    "dev:server": "cross-env NODE_ENV=development nodemon ./app-myblog.js",

    "test:build": "npm run clean && parallel-webpack --config ./build/test/webpack.config.test.js --display-used-exports --optimize-dedupe",
    "test:server": "cross-env NODE_ENV=production test=test nodemon ./app-myblog.js",
    "test:run": "npm run test:build && npm run test:server",

    "prod:build": "npm run clean && parallel-webpack --config ./build/prod/webpack.config.prod.js --display-used-exports --optimize-dedupe",
    "prod:server": "cross-env NODE_ENV=production nodemon ./app-myblog.js",
    "prod:run": "npm run prod:build && npm run prod:server",

    "pm2": "cross-env NODE_ENV=production pm2 start app-myblog.js"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "ccap": "^0.6.10",
    "cross-env": "^5.1.4",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "ioredis": "^3.2.2",
    "js-md5": "^0.7.3",
    "jsonwebtoken": "^8.2.1",
    "koa": "^2.5.0",
    "koa-bodyparser": "^4.2.0",
    "koa-logger": "^3.2.0",
    "koa-ratelimit": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^4.0.2",
    "koa-validate": "^1.0.7",
    "mysql2": "^1.5.2",
    "nodemailer": "^4.6.4",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "remark": "^9.0.0",
    "remark-react": "^4.0.1",
    "xss": "^0.3.7"
  },
  "devDependencies": {
    "asset-require-hook": "^1.2.0",
    "autoprefixer": "^8.2.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-import": "^1.4.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-remove-console": "^6.8.5",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-react-optimize": "^1.0.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "compression-webpack-plugin": "^1.1.11",
    "css-loader": "^0.28.11",
    "css-modules-require-hook": "^4.2.3",
    "cz-conventional-changelog": "^2.1.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "hard-source-webpack-plugin": "^0.6.4",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.0.7",
    "jest": "^22.4.3",
    "koa-webpack": "^3.0.2",
    "koa-webpack-dev-middleware": "^2.0.2",
    "koa-webpack-server": "^0.2.1",
    "mockjs": "^1.0.1-beta3",
    "parallel-webpack": "^2.3.0",
    "postcss": "^6.0.22",
    "postcss-easysprites": "^0.1.8",
    "postcss-icss-values": "^2.0.1",
    "postcss-loader": "^2.1.5",
    "precss": "^3.1.2",
    "react-hot-loader": "^4.0.0",
    "react-test-renderer": "^16.3.2",
    "should": "^13.2.1",
    "sprite-loader": "^0.3.7",
    "style-loader": "^0.20.3",
    "supertest": "^3.0.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.15",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2",
    "write-file-webpack-plugin": "^4.2.0"
  },
  "keywords": [
    "改版个人博客"
  ],
  "author": "garven",
  "license": "ISC",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0",
      [
        "env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "react-hot-loader/babel",
      "transform-runtime",
      "transform-class-properties",
      "transform-object-rest-spread",
      "syntax-dynamic-import",
      "transform-decorators-legacy"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "dist"
    ]
  }
}
